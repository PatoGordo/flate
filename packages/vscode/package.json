{
  "name": "flate",
  "displayName": "Flate",
  "description": "Dark Colored Theme for VS Code.",
  "version": "0.6.0",
  "publisher": "hiukky",
  "author": {
    "name": "Romullo",
    "email": "developermarsh@gmail.com",
    "url": "https://hiukky.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hiukky/flate/issues"
  },
  "homepage": "https://github.com/hiukky/flate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hiukky/flate"
  },
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Themes"
  ],
  "scripts": {
    "dev": "nodemon --watch themes scripts/build.ts",
    "build": "ts-node scripts/build.ts"
  },
  "contributes": {
    "themes": [
      {
        "label": "Flate",
        "uiTheme": "vs-dark",
        "path": "./dist/flate.json"
      },
      {
        "label": "Flate (Italic)",
        "uiTheme": "vs-dark",
        "path": "./dist/flate.italic.json"
      },
      {
        "label": "Flate (Bold)",
        "uiTheme": "vs-dark",
        "path": "./dist/flate.bold.json"
      },
      {
        "label": "Flate Arc",
        "uiTheme": "vs-dark",
        "path": "./dist/flate-arc.json"
      },
      {
        "label": "Flate Arc (Italic)",
        "uiTheme": "vs-dark",
        "path": "./dist/flate-arc.italic.json"
      },
      {
        "label": "Flate Arc (Bold)",
        "uiTheme": "vs-dark",
        "path": "./dist/flate-arc.bold.json"
      },
      {
        "label": "Flate Punk",
        "uiTheme": "vs-dark",
        "path": "./dist/flate-punk.json"
      },
      {
        "label": "Flate Punk (Italic)",
        "uiTheme": "vs-dark",
        "path": "./dist/flate-punk.italic.json"
      },
      {
        "label": "Flate Punk (Bold)",
        "uiTheme": "vs-dark",
        "path": "./dist/flate-punk.bold.json"
      }
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "@flate/core": "*"
  }
}
